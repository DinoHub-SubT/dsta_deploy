#!/bin/bash

# set the rosbag logging directory, in the docker container
export subt_rosbag_dir=/home/developer/bags

# must set 'enable_hardware' variable
export subt_ugv1_enable_hardware=false

# example options: 'urban_underground', 'simple_cave_01'
export subt_ugv1_gazebo_world=simple_cave_01

# -- env variables, that will stay persistent in the bash shell --

# export gazebo model path
export GAZEBO_MODEL_PATH=/home/developer/deploy_ws/src/ugv/sim/darpa/catkin/darpa_subt/subt_gazebo/models
# export or-tools library path, for gtest
export LD_LIBRARY_PATH=/home/developer/deploy_ws/src/ugv/sim/sensor_coverage_planner/or-tools/lib/:$LD_LIBRARY_PATH
