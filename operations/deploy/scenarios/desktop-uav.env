# specify all variables to export

# //////////////////////////////////////////////////////////////////////////////
# relative path to (from dockerfiles) the child docker compose file to nest
export CHILD_DOCKER_COMPOSE="uav/"

# //////////////////////////////////////////////////////////////////////////////
# Setup the bashrc roscore environment variables
export ROS_MASTER_IP="10.0.2.5"
export ROS_MASTER_HOSTNAME="az-uav"

# //////////////////////////////////////////////////////////////////////////////
# override any base IPs or hostnames and add to /etc/hosts in the docker container
export BASESTATION_IP="10.0.2.6"
export BASESTATION_HOSTNAME="basestation"

export UAV_IP="10.0.2.5"
export UAV_HOSTNAME="az-uav"

# //////////////////////////////////////////////////////////////////////////////
# set either 'x86-gpu', 'arm-gpu' or 'non-gpu' to toggle gpu passthru or not
export GPU_ENABLE_TYPE="x86-gpu"

# //////////////////////////////////////////////////////////////////////////////
# deployerbooks variables

# deployer docker config
export user=katarina
export host=katarina-GL65-9SE
